<form action="{% spaceless %}
{% if common.id %}
{{ path('audit') }}
{% else %}
{{ path('audit') }}
{% endif %}
{% endspaceless %}" method="post" {{ form_enctype(form) }}>
    <div class="embedded">
        <h4>3.Технические характеристики зданий</h4>
        <blockquote>
        <p class="lead">Для каждого здания</p>
        </blockquote>
        {{ form_errors(form.Buildings) }}
        <div>
            {% for building in form.Buildings %}
            <div>
                Здание №{{form_label(building)}}
                {{ form_row(building.name) }}
                {{ form_row(building.year) }}
                {{ form_row(building.floors) }}
                {{ form_row(building.height_ceilings) }}
                {{ form_row(building.height_building) }}
                {{ form_row(building.volume_building) }}
                {{ form_row(building.volume_heated) }}
                {{ form_row(building.area_total) }}
                {{ form_row(building.area_glazing) }}
                {{ form_row(building.perimeter_building) }}
                {{ form_row(building.depreciation_actual) }}
                {{ form_row(building.depreciation_physical) }}
                {{ form_row(building.Common) }}
            </div>
            {% endfor %}
        </div>
    </div>
    {{ form_rest(form) }}
</form>
<button class="btn btn-primary"><i class="icon-plus-sign icon-white"></i> Добавть здание</button>