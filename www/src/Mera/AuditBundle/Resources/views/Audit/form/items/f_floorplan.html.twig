{% extends 'MeraAuditBundle:Audit:form/file.html.twig' %}
{% block collapseTitle %}
    Поэтажный план здания
{% endblock %}
{% block collapseId %}FloorPlan{% endblock %}
{% block filesData %}
    Mera.files.FloorPlan=[
    {% for file in common.getFloorPlans() %}
        {
        name: "{{ file.name }}",
        size: "{{ file.size }}",
        type: "",
        url: "",
        thumbnail_url: "",
        delete_url: "",
        delete_type: "DELETE",
        hash: "{{ file.hash }}",
        }
    {% if not loop.last %},{% endif %}
    {% endfor %}
    ];
{% endblock %}


